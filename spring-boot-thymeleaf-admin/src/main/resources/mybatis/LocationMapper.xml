<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyx.mapper.LocationMapper">

	<select id="getMarkers" resultType="java.util.Map">
	SELECT * FROM WP_LOCATION 
	</select>
    
    <insert id="save" parameterType="com.zyx.model.PageData">
    insert into wp_location(LOCATION_ID,USER_ID,LATITUDE,LONGITUDE,TYPE) 
    values(#{LOCATION_ID},#{USER_ID},#{LATITUDE},#{LONGITUDE},#{TYPE})
    </insert>
    
</mapper>